// this file is auto-generated by the generate-db-schema. do not make any manual changes

datasource db { 
  provider = "postgresql"
  url = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}
  

enum ProductStatus  {
  SHIPPED
  ORDERED 
  IDLE
}

model Product {
  id Int @id @default(autoincrement())
  name String
  price Int
  status ProductStatus @default(IDLE)
  shipping Shipping? @relation(fields: [shipping_id], references: [id])
  shipping_id Int?
  image String?
  images String[]
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  category String?
}

model Shipping {
  id        Int      @id @default(autoincrement())
  from String
  to String
  products Product[]
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}